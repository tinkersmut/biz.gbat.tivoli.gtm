insert into ECCN (eccn, parent, iscategory, isitar, description) values ('7A116', '7A', 0, 0, 'Flight control systems (hydraulic, mechanical, electro-optical, or electro-mechanical flight control systems (including fly-by-wire systems) and attitude control equipment) designed or modified for "missiles". (These items are subject to the export licensing authority of the U.S. Department of State, Directorate of Defense Trade Controls. See 22 CFR part 121.)');
