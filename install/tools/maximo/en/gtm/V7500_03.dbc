insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('6A001.a.1.e.3', '6A001', 0, 0, 'Transmitted pulse signal bandwidth exceeding 3 kHz;  ', ' Note:   For 6A001.a.1.e, where multiple detection ranges are specified for various environments, the greatest detection range is used.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('6A001.a.2', '6A001', 0, 0, 'Passive systems, equipment and specially designed components therefor, as follows:  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('6A001.a.2.a', '6A001', 0, 0, 'Hydrophones having any of the following:  ', ' Note:   The control status of hydrophones specially designed for other equipment is determined by the control status of the other equipment.    Technical Note:    <I>Hydrophones consist of one or more sensing elements producing a single acoustic output channel. Those that contain multiple elements can be referred to as a hydrophone group. </I>  ');
