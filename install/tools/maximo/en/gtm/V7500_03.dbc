insert into ECCN (eccn, parent, iscategory, isitar, description) values ('7A006', '7A', 0, 0, 'Airborne altimeters operating at frequencies other than 4.2 to 4.4 GHz inclusive and having any of the following ( <I>see </I><B>List of Items Controlled).</B>');
