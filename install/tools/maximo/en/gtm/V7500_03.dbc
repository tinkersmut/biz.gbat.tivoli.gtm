insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A', '5', 1, 0, 'Systems, Equipment and Components');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001', '5A', 0, 0, 'Telecommunications systems, equipment, components and accessories, as follows (see List of Items Controlled).');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.a', '5A001', 0, 0, 'Any type of telecommunications equipment having any of the following characteristics, functions or features:');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.a.1', '5A001', 0, 0, 'Specially designed to withstand transitory electronic effects or electromagnetic pulse effects, both arising from a nuclear explosion;  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.a.2', '5A001', 0, 0, 'Specially hardened to withstand gamma, neutron or ion radiation; <I>or </I>  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A001.a.3', '5A001', 0, 0, 'Specially designed to operate outside the temperature range from 218 K (&minus;55 &deg;C) to 397 K (124 &deg;C);  ', ' Note:    <I>5A001.a.3 applies only to electronic equipment. </I>    Note:    <I>5A001.a.2 and 5A001.a.3 do not apply to equipment designed or modified for use on board satellites. </I>  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.b', '5A001', 0, 0, 'Telecommunication systems and equipment, and specially designed components and accessories therefor, having any of the following characteristics, functions or features:  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.b.1 ', '5A001', 0, 0, 'Being underwater untethered communications systems having any of the following:  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.b.1.a', '5A001', 0, 0, 'An acoustic carrier frequency outside the range from 20 kHz to 60 kHz;  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.b.1.b', '5A001', 0, 0, 'Using an electromagnetic carrier frequency below 30 kHz; <I>or </I>  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.b.1.c', '5A001', 0, 0, 'Using electronic beam steering techniques; <I>or </I>  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.b.1.d', '5A001', 0, 0, 'Using "lasers" or light-emitting diodes (LEDs) with an output wavelength greater than 400 nm and less than 700 nm, in a "local area network";  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.b.2', '5A001', 0, 0, 'Being radio equipment operating in the 1.5 MHz to 87.5 MHz band and having all of the following:  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.b.2.a', '5A001', 0, 0, 'Automatically predicting and selecting frequencies and "total digital transfer rates" per channel to optimize the transmission; <I>and </I>  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.b.2.b', '5A001', 0, 0, 'Incorporating a linear power amplifier configuration having a capability to support multiple signals simultaneously at an output power of 1 kW or more in the frequency range of 1.5 MHz or more but less than 30 MHz, or 250 W or more in the frequency range of 30 MHz or more but not exceeding 87.5 MHz, over an "instantaneous bandwidth" of one octave or more and with an output harmonic and distortion content of better than &minus;80 dB;  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.b.3', '5A001', 0, 0, 'Being radio equipment employing "spread spectrum" techniques, including "frequency hopping" techniques, not controlled in 5A001.b.4 and having any of the following:  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.b.3.a', '5A001', 0, 0, 'User programmable spreading codes; <I>or </I>  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A001.b.3.b', '5A001', 0, 0, 'A total transmitted bandwidth which is 100 or more times the bandwidth of any one information channel and in excess of 50 kHz;  ', ' Note:    <I>5A001.b.3.b does not control radio equipment specially designed for use with any of the following: </I>      <I>a. Civil cellular radio-communications systems; or </I>      <I>b. Fixed or mobile satellite Earth stations for commercial civil telecommunications. </I>    Note:    <I>5A001.b.3 does not control equipment operating at an output power of 1 W or less. </I>  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.b.4', '5A001', 0, 0, 'Being radio equipment employing ultra-wideband modulation techniques, having user programmable channelizing codes, scrambling codes, or network identification codes and having any of the following:  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.b.4.a', '5A001', 0, 0, 'A bandwidth exceeding 500 MHz; <I>or </I>  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.b.4.b', '5A001', 0, 0, 'A "fractional bandwidth" of 20% or more;  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.b.5', '5A001', 0, 0, 'Being digitally controlled radio receivers having all of the following:  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.b.5.a', '5A001', 0, 0, 'More than 1,000 channels;  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.b.5.b', '5A001', 0, 0, 'A "frequency switching time" of less than 1 ms;  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.b.5.c', '5A001', 0, 0, 'Automatic searching or scanning of a part of the electromagnetic spectrum; <I>and </I>  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A001.b.5.d', '5A001', 0, 0, 'Identification of the received signals or the type of transmitter; <I>or </I>  ', ' Note:    <I>5A001.b.5 does not control radio equipment specially designed for use with civil cellular radio-communications systems. </I>  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A001.b.6', '5A001', 0, 0, 'Employing functions of digital "signal processing" to provide voice coding output at rates of less than 2,400 bit/s.  ', ' Technical Notes:    <I>1. For variable rate voice coding, 5A001.b.6 applies to the voice coding output of continuous speech. </I>  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A001.c', '5A001', 0, 0, 'Optical fibers of more than 500 m in length and specified by the manufacturer as being capable of withstanding a proof test tensile stress of 2 &times; 10<SUP>9</SUP> N/m<SUP>2</SUP> or more;  ', ' Technical Note:   Proof Test: on-line or off-line production screen testing that dynamically applies a prescribed tensile stress over a 0.5 to 3 m length of fiber at a running rate of 2 to 5 m/s while passing between capstans approximately 150 mm in diameter. The ambient temperature is a nominal 293 K (20 &deg;C) and relative humidity 40%. Equivalent national standards may be used for executing the proof test.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A001.d', '5A001', 0, 0, '"Electronically steerable phased array antennae" operating above 31.8 GHz;  ', ' Note:    <I>5A001.d does not control "electronically steerable phased array antennae" for landing systems with instruments meeting ICAO standards covering Microwave Landing Systems (MLS). </I>  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.e', '5A001', 0, 0, 'Radio direction finding equipment operating at frequencies above 30 MHz and having all of the following, and specially designed components therefor:  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.e.1', '5A001', 0, 0, '"Instantaneous bandwidth" of 10 MHz or more; and  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.e.2', '5A001', 0, 0, 'Capable of finding a Line Of Bearing (LOB) to non-cooperating radio transmitters with a signal duration of less than 1 ms;  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.f', '5A001', 0, 0, 'Jamming equipment specially designed or modified to intentionally and selectively interfere with, deny, inhibit, degrade or seduce mobile telecommunication services and perform any of the following, and specially designed components therefor:  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.f.1', '5A001', 0, 0, 'Simulate the functions of Radio Access Network (RAN) equipment;  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.f.2', '5A001', 0, 0, 'Detect and exploit specific characteristics of the mobile telecommunications protocol employed ( <I>e.g., </I>GSM); or  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A001.f.3', '5A001', 0, 0, 'Exploit specific characteristics of the mobile telecommunications protocol employed ( <I>e.g., </I>GSM);  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A001.g', '5A001', 0, 0, 'Passive Coherent Location (PCL) systems or equipment, specially designed for detecting and tracking moving objects by measuring reflections of ambient radio frequency emissions, supplied by non-radar transmitters.  ', ' Technical Note:   Non-radar transmitters may include commercial radio, television or cellular telecommunications base stations.    Note:   5A001.g does not control:     a. Radio-astronomical equipment; or     b. Systems or equipment, that require any radio transmission from the target.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A001.h', '5A001', 0, 0, 'Radio Frequency (RF) transmitting equipment designed or modified for prematurely activating or preventing the initiation of Improvised Explosive Devices (IEDs).  ', ' N.B.:   See also ECCN 5A001.f and Category XI of the International Traffic in Arms Regulations (ITAR) (22 CFR Parts 120&ndash;130).  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A001.i', '5A001', 0, 0, 'Systems or equipment, specially designed or modified to intercept and process the air interface of mobile telecommunications, and specially designed components therefor.  ', ' Note:    <I>5A001.i does not apply to equipment designed for mobile telecommunications network operators, or for the "development" or "production" of mobile telecommunications equipment or systems </I>.    Technical Note:    <I>For the purposes of 5A001.i, mobile telecommunications refers to the following telecommunications protocols or standards: GSM, GSM&ndash;R, GPRS, IMT&ndash;2000, PMR (Professional Mobile Radio), Inmarsat, Iridium, Thuraya, VSAT or ACES </I>.    N.B.:    <I>See also 5A001.f, 5A980, and the U.S. Munitions List (22 CFR part 121) </I>.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A101', '5A', 0, 0, 'Telemetering and telecontrol equipment, including ground equipment, designed or modified for unmanned aerial vehicles or rocket systems (including ballistic missile systems, space launch vehicles, sounding rockets, cruise missile systems, target drones, and reconnaissance drones) capable of a maximum "range" equal to or greater than 300km.');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A980', '5A', 0, 0, 'Devices primarily useful for the surreptitious interception of wire, oral, or electronic communications, other than those controlled under 5A001.i; and parts and accessories therefor');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991', '5A', 0, 0, 'Telecommunication equipment, not controlled by 5A001.');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.a', '5A991', 0, 0, 'Any type of telecommunications equipment, not controlled by 5A001.a, specially designed to operate outside the temperature range from 219 K (&minus;54 &deg;C) to 397 K (124 &deg;C).');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A991.b', '5A991', 0, 0, 'Telecommunication transmission equipment and systems, and specially designed components and accessories therefor, having any of the following characteristics, functions or features:  ', ' Note:   Telecommunication transmission equipment:     a. Categorized as follows, or combinations thereof:     1. Radio equipment ( <I>e.g. </I>, transmitters, receivers and transceivers);     2. Line terminating equipment;     3. Intermediate amplifier equipment;     4. Repeater equipment;     5. Regenerator equipment;     6. Translation encoders (transcoders);     7. Multiplex equipment (statistical mutiplex included);     8. Modulators/demodulators (modems);     9. Transmultiplex equipment (see CCITT Rec. G701);     10. "Stored program controlled" digital crossconnection equipment;     11. "Gateways" and bridges;     12. "Media access units"; <I>and </I>     b. Designed for use in single or multi-channel communication via any of the following:     1. Wire (line);     2. Coaxial cable;     3. Optical fiber cable;     4. Electromagnetic radiation; or     5. Underwater acoustic wave propagation.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A991.b.1', '5A991', 0, 0, 'Employing digital techniques, including digital processing of analog signals, and designed to operate at a "digital transfer rate" at the highest multiplex level exceeding 45 Mbit/s or a "total digital transfer rate" exceeding 90 Mbit/s;  ', ' Note:   5A991.b.1 does not control equipment specially designed to be integrated and operated in any satellite system for civil use.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.b.2', '5A991', 0, 0, 'Modems using the "bandwidth of one voice channel" with a "data signaling rate" exceeding 9,600 bits per second;  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.b.3', '5A991', 0, 0, 'Being "stored program controlled" digital cross connect equipment with "digital transfer rate" exceeding 8.5 Mbit/s per port.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.b.4', '5A991', 0, 0, 'Being equipment containing any of the following:  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.b.4.a', '5A991', 0, 0, '"Network access controllers" and their related common medium having a "digital transfer rate" exceeding 33 Mbit/s; <I>or </I>  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A991.b.4.b', '5A991', 0, 0, '"Communication channel controllers" with a digital output having a "data signaling rate" exceeding 64,000 bit/s per channel;  ', ' Note:   If any uncontrolled equipment contains a "network access controller", it cannot have any type of telecommunications interface, except those described in, but not controlled by 5A991.b.4.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.b.5', '5A991', 0, 0, 'Employing a "laser" and having any of the following characteristics:  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.b.5.a', '5A991', 0, 0, 'A transmission wavelength exceeding 1,000 nm; <I>or </I>  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A991.b.5.b', '5A991', 0, 0, 'Employing analog techniques and having a bandwidth exceeding 45 MHz;  ', ' Note:   5A991.b.5.b does not control commercial TV systems.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.b.5.c', '5A991', 0, 0, 'Employing coherent optical transmission or coherent optical detection techniques (also called optical heterodyne or homodyne techniques);  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.b.5.d', '5A991', 0, 0, 'Employing wavelength division multiplexing techniques; <I>or </I>  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.b.5.e', '5A991', 0, 0, 'Performing "optical amplification";  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.b.6', '5A991', 0, 0, 'Radio equipment operating at input or output frequencies exceeding:  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.b.6.a', '5A991', 0, 0, '31 GHz for satellite-earth station applications; <I>or </I>  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A991.b.6.b', '5A991', 0, 0, '26.5 GHz for other applications;  ', ' Note:   5A991.b.6. does not control equipment for civil use when conforming with an International Telecommunications Union (ITU) allocated band between 26.5 GHz and 31 GHz.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.b.7', '5A991', 0, 0, 'Being radio equipment employing any of the following:  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.b.7.a', '5A991', 0, 0, 'Quadrature-amplitude-modulation (QAM) techniques above level 4 if the "total digital transfer rate" exceeds 8.5 Mbit/s;  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.b.7.b', '5A991', 0, 0, 'QAM techniques above level 16 if the "total digital transfer rate" is equal to or less than 8.5 Mbit/s;  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.b.7.c', '5A991', 0, 0, 'Other digital modulation techniques and having a "spectral efficiency" exceeding 3 bit/s/Hz; or  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A991.b.7.d', '5A991', 0, 0, 'Operating in the 1.5 MHz to 87.5 MHz band and incorporating adaptive techniques providing more than 15 dB suppression of an interfering signal.  ', ' Notes:   1. 5A991.b.7 does not control equipment specially designed to be integrated and operated in any satellite system for civil use.     2. 5A991.b.7 does not control radio relay equipment for operation in an ITU allocated band:     a. Having any of the following:     a.1. Not exceeding 960 MHz; or     a.2. With a "total digital transfer rate" not exceeding 8.5 Mbit/s; and     b. Having a "spectral efficiency" not exceeding 4 bit/s/Hz.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A991.c', '5A991', 0, 0, '"Stored program controlled" switching equipment and related signaling systems, having any of the following characteristics, functions or features, and specially designed components and accessories therefor:  ', ' Note:   Statistical multiplexers with digital input and digital output which provide switching are treated as "stored program controlled" switches.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.c.1', '5A991', 0, 0, '"Data (message) switching" equipment or systems designed for "packet-mode operation" and assemblies and components therefor, n.e.s.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A991.c.2', '5A991', 0, 0, '[Reserved]  ', ' Note:   5A991.c does not preclude the evaluation and appropriate actions taken by the receiving switch or unrelated user message traffic on a D channel of ISDN.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.c.3', '5A991', 0, 0, 'Routing or switching of "datagram" packets;  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A991.c.4', '5A991', 0, 0, '[Reserved]  ', ' Note:   The restrictions in 5A991.c.3 do not apply to networks restricted to using only "network access controllers" or to "network access controllers" themselves.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A991.c.5', '5A991', 0, 0, 'Multi-level priority and pre-emption for circuit switching;  ', ' Note:   5A991.c.5 does not control single-level call preemption.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.c.6', '5A991', 0, 0, 'Designed for automatic hand-off of cellular radio calls to other cellular switches or automatic connection to a centralized subscriber data base common to more than one switch;  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.c.7', '5A991', 0, 0, 'Containing "stored program controlled" digital cross connect equipment with "digital transfer rate" exceeding 8.5 Mbit/s per port.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.c.8', '5A991', 0, 0, '"Common channel signaling" operating in either non-associated or quasi-associated mode of operation;  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A991.c.9', '5A991', 0, 0, '"Dynamic adaptive routing";  ', ' Note:   5A991.c.10 does not control packet switches or routers with ports or lines not exceeding the limits in 5A991.c.10.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.c.10', '5A991', 0, 0, 'Being packet switches, circuit switches and routers with ports or lines exceeding any of the following:  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A991.c.10.a', '5A991', 0, 0, 'A "data signaling rate" of 64,000 bit/s per channel for a "communications channel controller"; <I>or </I>  ', ' Note:   5A991.c.10.a does not control multiplex composite links composed only of communication channels not individually controlled by 5A991.b.1.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.c.10.b', '5A991', 0, 0, 'A "digital transfer rate" of 33 Mbit/s for a "network access controller" and related common media;  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.c.11', '5A991', 0, 0, '"Optical switching";  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.c.12', '5A991', 0, 0, 'Employing "Asynchronous Transfer Mode" ("ATM") techniques.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.d', '5A991', 0, 0, 'Optical fibers and optical fiber cables of more than 50 m in length designed for single mode operation;  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.e', '5A991', 0, 0, 'Centralized network control having all of the following characteristics:  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.e.1', '5A991', 0, 0, 'Receives data from the nodes; <I>and </I>  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5A991.e.2', '5A991', 0, 0, 'Process these data in order to provide control of traffic not requiring operator decisions, and thereby performing "dynamic adaptive routing";  ', ' Note:   5A991.e does not preclude control of traffic as a function of predictable statistical traffic conditions.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.f', '5A991', 0, 0, 'Phased array antennae, operating above 10.5 GHz, containing active elements and distributed components, and designed to permit electronic control of beam shaping and pointing, except for landing systems with instruments meeting International Civil Aviation Organization (ICAO) standards (microwave landing systems (MLS)).  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.g', '5A991', 0, 0, 'Mobile communications equipment, n.e.s., and assemblies and components therefor; or  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5A991.h', '5A991', 0, 0, 'Radio relay communications equipment designed for use at frequencies equal to or exceeding 19.7 GHz and assemblies and components therefor, n.e.s.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5B', '5', 1, 0, 'Test, Inspection and Production Equipment');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5B001', '5B', 0, 0, 'Telecommunication test, inspection and production equipment, components and accessories, as follows (See List of Items Controlled).');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5B001.a', '5B001', 0, 0, 'Equipment and specially designed components or accessories therefor, specially designed for the "development", "production" or "use" of equipment, functions or features, controlled by 5A001;', ' Note:   5B001.a does not control optical fiber characterization equipment.  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5B001.b', '5B001', 0, 0, 'Equipment and specially designed components or accessories therefor, specially designed for the "development" of any of the following telecommunication transmission or switching equipment:  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5B001.b.1', '5B001', 0, 0, '[Reserved]  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5B001.b.2', '5B001', 0, 0, 'Equipment employing a "laser" and having any of the following:  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5B001.b.2.a', '5B001', 0, 0, 'A transmission wavelength exceeding 1750 nm;  ');
insert into ECCN (eccn, parent, iscategory, isitar, description) values ('5B001.b.2.b', '5B001', 0, 0, 'Performing "optical amplification" using praseodymium-doped fluoride fiber amplifiers (PDFFA);  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5B001.b.2.c', '5B001', 0, 0, 'Employing coherent optical transmission or coherent optical detection techniques; <I>or </I>  ', ' Note:    <I>5B001.b.2.c applies to equipment specially designed for the "development" of systems using an optical local oscillator in the receiving side to synchronize with a carrier "laser." </I>    Technical Note:    <I>For the purpose of 5B001.b.2.c, these techniques include optical heterodyne, homodyne or intradyne techniques. </I>  ');
insert into ECCN (eccn, parent, iscategory, isitar, description, notes) values ('5B001.b.2.d', '5B001', 0, 0, 'Employing analog techniques and having a bandwidth exceeding 2.5 GHz; or  ', ' Note:    <I>5B001.b.2.d. does not include equipment specially designed for the "development" of commercial TV systems. </I>  ');
